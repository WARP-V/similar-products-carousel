    "nodemon": "^1.18.4",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.3.2",
    "style-loader": "^0.23.1",
    "supertest": "^3.3.0",
    "webpack": "2.6.1"
